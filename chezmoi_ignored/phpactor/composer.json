{
  "name": "grota/phpactor",
  "authors": [
    {
      "name": "Giuseppe Rota"
    }
  ],
  "require": {
    "fenetikm/autoload-drupal": "dev-master"
  },
  "config": {
    "allow-plugins": true
  },
  "extra": {
    "autoload-drupal": {
      "modules": [
        "../$path_to_web/core/modules",
        "../$path_to_web/modules/custom",
        "../$path_to_web/modules/contrib"
      ],
      "classmap": [
        "../$path_to_web/core/lib"
      ]
    }
  }
}
