<gconfentryfile>
  <entrylist base="/apps/hamster-indicator">
    <entry>
      <key>icon_glow</key>
      <schema_key>/schemas/apps/hamster-indicator/icon_glow</schema_key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>label_length</key>
      <schema_key>/schemas/apps/hamster-indicator/label_length</schema_key>
      <value>
        <int>20</int>
      </value>
    </entry>
    <entry>
      <key>show_label</key>
      <schema_key>/schemas/apps/hamster-indicator/show_label</schema_key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
