" vim: set ft=vim:
set defsearch=gg
set hlsearch
set tabnumbers
autocmd LocationChange .* js modes.passAllKeys = /mail\.google\.com/.test(buffer.URL)
nnoremap <Esc> <C-c>
nnoremap j 7j
nnoremap k 7k
nnoremap e ;F
nnoremap S :tabopen gg 
nnoremap s :open gg 
set complete=lbs
set gui=nonavigation

set mainecoon=a
